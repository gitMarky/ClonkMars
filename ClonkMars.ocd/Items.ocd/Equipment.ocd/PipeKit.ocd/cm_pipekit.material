material cm_pipekit
{
	receive_shadows on 

	technique
	{
		pass 
		{
			ambient 1.0 1.0 1.0 1.0
			diffuse 0.8 0.8 0.8 1.0
			specular 1.0 1.0 1.0 1.0 25
			emissive 0.0 0.0 0.0 1.0

			texture_unit
			{
				texture pipekit.png
				tex_address_mode wrap
				colour_op modulate
				filtering trilinear
			}
		}
	}
}

